{
  "name": "mantle-gain-mantle",
  "version": "1.0.0",
  "description": "Mantle-Gain DeFi Protocol deployed on Mantle Network",
  "main": "index.js",
  "scripts": {
    "compile": "npx hardhat compile",
    "test": "npx hardhat test",
    "test:gas": "REPORT_GAS=true npx hardhat test",
    "coverage": "npx hardhat coverage",
    "node": "npx hardhat node",
    "deploy:local": "npx hardhat run scripts/deploy.js --network localhost",
    "deploy:mantle": "npx hardhat run scripts/deploy.js --network mantle",
    "deploy:mantle-testnet": "npx hardhat run scripts/deploy.js --network mantleTestnet",
    "verify:mantle": "npx hardhat verify --network mantle",
    "verify:mantle-testnet": "npx hardhat verify --network mantleTestnet",
    "clean": "npx hardhat clean",
    "size": "npx hardhat size-contracts",
    "flatten": "npx hardhat flatten",
    "check": "npx hardhat check",
    "lint": "solhint 'contracts/**/*.sol'",
    "lint:fix": "solhint 'contracts/**/*.sol' --fix",
    "prettier": "prettier --write 'contracts/**/*.sol'",
    "slither": "slither .",
    "docs": "npx hardhat docgen"
  },
  "keywords": [
    "defi",
    "yield-farming",
    "mantle",
    "ethereum",
    "smart-contracts",
    "solidity"
  ],
  "author": "Mantle-Gain Team",
  "license": "MIT",
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.0",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "chai": "^4.2.0",
    "ethers": "^5.4.7",
    "hardhat": "^2.14.0",
    "hardhat-gas-reporter": "^1.0.8",
    "hardhat-contract-sizer": "^2.8.0",
    "hardhat-deploy": "^0.11.25",
    "hardhat-docgen": "^1.3.0",
    "solidity-coverage": "^0.8.1",
    "typechain": "^8.1.0",
    "dotenv": "^16.0.0",
    "prettier": "^2.8.0",
    "prettier-plugin-solidity": "^1.1.0",
    "solhint": "^3.4.0",
    "slither-analyzer": "^0.1.0"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.8.0",
    "@openzeppelin/contracts-upgradeable": "^4.8.0"
  }
}